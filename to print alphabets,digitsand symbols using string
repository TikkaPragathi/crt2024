#include <stdio.h>
#include<string.h>
int main() {
  char str[100];
  int i,alp=0,dig=0,sym=0;
  scanf("%[^\n]s",str);
  for(i=0;str[i]!='\0';i++)
  {
      if((str[i]>=65 && str[i]<=90) ||(str[i]>=97 && str[i]<=122))
      alp++;
      else if(str[i]>=48 && str[i]<=57)
      dig++;
      else
      sym++;
  }
  printf("%d\n%d\n%d",alp,dig,sym);
    return 0;
}
Output:
12abd % gg*
5  //(alphabets)
2   // (digits)
4   //(symbols including space)
