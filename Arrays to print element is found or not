#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int sno;
    scanf("%d",&sno);
    for(i=0;i<n;i++)
    {
        if(sno==a[i])
        {
            printf("Element is found");
            break;
        }
    }
