#include <stdio.h>
#include<math.h>

int main() {
  
 float a,b,c;
 scanf("%f%f%f",&a,&b,&c);
 float d=b*b-4*a*c;
 if(d<0)
 {
     printf("Roots are imaginary");
 }
 else
 {
     float r1=(-b+sqrt(d))/(2*a);
     float r2=(-b-sqrt(d))/(2*a);
     printf("Root 1=%f\n",r1);
     printf("Root 2=%f\n",r2);
 }
  
    return 0;
}

Output:
1 4 3
Root 1=-1.000000
Root 2=-3.000000

1 2 3
Roots are imaginary
