#include <stdio.h>

int main() {
  
 int a,b,c;
 scanf("%d%d%d",&a,&b,&c);
 if(a>b && a>c)
 {
     printf("%d",a);
 }
 else
 if(b>c)
 {
     printf("%d",b);
 }
 else
 {
     printf("%d",c);
 }
    return 0;
}

Output:
a has greater value
12
3
5
12
//b has greater value
13 45 5
45
//c is greater
13 45 5
45
