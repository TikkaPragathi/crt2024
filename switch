#include <stdio.h>

int main() {
  
 int a,b,c;
 scanf("%d%d%d",&a,&b,&c);
// int c;
 //scanf("%d",&c);
 switch(c)
 {
 case 1:
   printf("%d",a+b);
   break;
   case 2:
   printf("%d",a-b);
   break;
   case 3:
   printf("%d",a*b);
   break;
   case 4:
   printf("%d",a/b);
   break;
   default:
    printf("invalid choice");
 }
    return 0;
}

Output:

1 2 1
3
