#include <stdio.h>

int main() {
    int n,i,c=0;
    scanf("%d",&n);
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
            c++;
            break;
        }
    }
    if(c==0)
    printf("Prime no");
    else
     printf(" Not a Prime no");
    return 0;
}
Output:

3
Prime no
4
 Not a Prime no
